<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cofitost.fcfood20">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_f"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseResultActivity"
            android:label="@string/title_activity_choose_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChooseActivity"
            android:label="@string/title_activity_choose"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChargeActivity"
            android:label="@string/title_activity_charge"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChooseAddActivity"
            android:label="@string/title_activity_choose_add"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowChooseDataActivity"
            android:label="@string/title_activity_show_choose_data"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>